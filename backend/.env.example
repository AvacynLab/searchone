# Copy to `.env` and fill in real values. Never commit secrets.

# LLM provider: "openrouter" or "local"
PROVIDER=openrouter

# OpenRouter (used when PROVIDER=openrouter)
OPENROUTER_API_KEY=sk-or-xxxxx
OPENROUTER_MODEL=arcee-ai/trinity-mini:free
# OPENROUTER_SITE=https://yourdomain.example
# OPENROUTER_TITLE=SearchOne Local

# Local LM Studio (used when PROVIDER=local)
# LMSTUDIO_URL=http://127.0.0.1:1234/v1/chat/completions
# LMSTUDIO_MODEL=your-local-model-name

# Embeddings
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Redis
REDIS_URL=redis://localhost:2002

# SearxNG
SEARXNG_URL=http://localhost:2003

# Long-run / orchestration limits
SEARCHONE_MAX_EMPTY_ITERS=0
REPLAN_MIN_EVIDENCE=0
REPLAN_MIN_COVERAGE=0.0
AGENT_TOKEN_BUDGET=0
# SEARCHONE_ACT_TIMEOUT=0
# SEARCHONE_COUNCIL_TIMEOUT=0
# SEARCHONE_RETRIEVAL_ONLY_THRESHOLD=2
# SEARCHONE_RETRIEVAL_ONLY_TOPK=8
# SEARCHONE_ESTIMATED_COST_PER_TOKEN=0

# Web search
# SEARCHONE_WEB_SEARCH_ENDPOINT=http://localhost:2003/search
SEARCHONE_WEB_SEARCH_TIMEOUT=8
SEARCHONE_WEB_FETCH_TIMEOUT=10
SEARCHONE_WEB_FETCH_MAX_CHARS=6000

# Rerank / embedder toggles
# SEARCHONE_SIMPLE_EMBEDDER=
# SEARCHONE_FORCE_ST=
# SEARCHONE_ENABLE_RERANK=
# SEARCHONE_CROSS_ENCODER_MODEL=cross-encoder/ms-marco-MiniLM-L-6-v2

# Scheduler / jobs
SEARCHONE_SCHEDULER_INTERVAL=0

# Tracing / logging
# SENTRY_ENVIRONMENT=local
# SEARCHONE_TOOL_TIMEOUT=8
# SEARCHONE_TOOL_HTTP_TIMEOUT=10
